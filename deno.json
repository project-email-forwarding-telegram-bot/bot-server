{
    "name": "bot-server",
    "version": "1.0.0",
    "tasks": {
        "dev": "deno run --env-file --allow-env --allow-net --watch src/main.ts"
    },
    "imports": {
        "@std/assert": "jsr:@std/assert@1",
        "@upstash/redis": "npm:@upstash/redis@^1.35.4"
    },
    "fmt": {
        "indentWidth": 4,
        "lineWidth": 120
    },
    "permissions": {
        "default": {
            "env": true,
            "read": true,
            "net": true
        }
    }
}
